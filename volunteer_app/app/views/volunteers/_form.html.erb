<%= form_for @volunteer do |form| %>
	First Name: <%= form.text_field :first_name %></br>
	Last Name: <%= form.text_field :last_name %></br>
	Email: <%= form.text_field :email%></br>
	Phone: <%= form.text_field :phone %></br>
	Image: <%= form.text_field :img_url %></br>
	Program Interests: <%= form.text_field :area_interest %></br>
	Languages Spoken: <%= form.text_field :languages %></br>
	<%= form.submit %>
<% end %>